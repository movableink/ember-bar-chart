<svg role="img" height={{this.height}} ...attributes>
  {{yield (hash
    Bar=(component 'bar-chart/bar'
      height=@barHeight
      maxValue=this.maxValue
      reportValue=this.receiveValue
      setBarPosition=this.setBarPosition
      onBarDestroy=this.handleDestroyedBar
      onRectDestroy=this.handleDestroyedRect
    )
  )}}
</svg>